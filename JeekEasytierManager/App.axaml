<Application
    x:Class="JeekEasytierManager.App"
    xmlns="https://github.com/avaloniaui"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:dialogHost="clr-namespace:DialogHostAvalonia;assembly=DialogHost.Avalonia"
    RequestedThemeVariant="Default">
    <!--
        "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available
        options.
    -->

    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceInclude Source="Style/Colors.axaml" />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>

    <Application.Styles>
        <FluentTheme />
        <dialogHost:DialogHostStyles />
        <StyleInclude Source="Style/Styles.axaml" />
    </Application.Styles>
</Application>
